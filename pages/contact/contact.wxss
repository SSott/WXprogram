.avatar{
  width: 100%;
  height: 400rpx;
  background-color: #FF5511;
}
.avatar-image{
  height: 220rpx;
  width: 220rpx;
  border: 10rpx solid rgba(0,0,0,.1);
  border-radius:50%; 
}

.cell{
  font-size: 16px;
}
.cell-item{
  border-bottom: 1rpx solid #ddd;
  padding: 20rpx 30rpx;  
}
.divider{
  height: 20rpx;
  background-color: #f1f1f1;
}
