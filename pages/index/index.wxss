.index-nav image{
  width: 80rpx;
  height: 80rpx;
}
.index-nav .nav-item {
  width: 33.33333%;
  height: 200rpx;
  border-bottom: 1rpx solid #ccc;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
  
}
.index-nav .nav-item::after{
  content: "";
  width: 1rpx;
  height: 100%;
  background-color: #ccc;
  position: absolute;
  right: 0;
  top: 0;
}
.index-nav .nav-item:nth-child(3n)::after{
  width: 0; 
}
